'#***************************************************************************************************************************************
'# Function/Sub Name              : func_Utility_Delete_Excel_Sheet(strFileName , strSheetName)
'# Purpose                        : To Delete excel row
'# Input Parameters               :	strFileName , strSheetName
'# Output Parameters              : Boolean Value(True/False)
'# Author                         : Preeti Singh
'# Date Created[dd-mon-yy]    	  : 10-Jan-17
'# Modified By                    :
'# Date Modified[dd-mon-yy]  	  :
'#***************************************************************************************************************************************

Function func_Utility_Delete_Excel_Sheet(strFileName , strSheetName)

	
	
	Dim objExcel , objWorkbook , objWorkSheet ,WorkSheetName ,strWorkSheetName 
	
	Set objExcel = CreateObject("Excel.Application")

	Set objWorkbook = objExcel.Workbooks.Open(strFileName)

	Set objWorkSheet = objWorkbook.WorkSheets(strSheetName)

	objExcel.displayalerts = False

	For each WorkSheetName in objWorkbook.worksheets

		strWorkSheetName =  WorkSheetName.name

		If Instr(LCase(strWorkSheetName),LCase(strSheetName)) > 0 Then

			WorkSheetName.Delete
		End If
	Next 

	objExcel.displayalerts = true

	objWorkbook.Save

	objWorkbook.Close

	objExcel.Application.Quit

	Set objExcel = Nothing

End Function

'# ********************************************************************************************
'# Function/Sub Name        : func_utility_createandgetFileObj
'# Purpose                  : to create a file for appending and return file object to write
'# Input Parameters         : file name
'# Output Parameters        : file object
'# Author			        : Kiran Londhe
'# Date Created[dd-mon-yy]  : 16-Feb-2017
'# Modified By				:
'# Date Modified[dd-mon-yy] :
'# ********************************************************************************************

Public Function func_utility_createandgetFileObj(l_str_filename)

Const Forreading = 1, ForWriting = 2, ForAppending = 8

   Set fso = CreateObject("Scripting.FileSystemObject")
	   If fso.FileExists (l_str_filename) Then
		  Set f = fso.GetFile(l_str_filename)
		  Set func_utility_createandgetFileObj = f.OpenAsTextStream(ForAppending, TristateTrue)
	   Else
		  fso.CreateTextFile(l_str_filename)
		  Set f = fso.GetFile(l_str_filename)
		  Set func_utility_createandgetFileObj = f.OpenAsTextStream(ForAppending, TristateTrue)
	   End if 

End Function


