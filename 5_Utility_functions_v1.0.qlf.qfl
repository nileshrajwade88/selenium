'#***************************************************************************************************************************************
'# Function/Sub Name              : func_Utility_Read_TextFile(strFileName , )
'# Purpose                        : To Copy File from one location to another location
'# Input Parameters               :	strFileName , strWord
'# Output Parameters              : Boolean Value(True/False)
'# Author                         : Preeti Singh
'# Date Created[dd-mon-yy]    	  : 10-Jan-17
'# Modified By                    :
'# Date Modified[dd-mon-yy]  	  :
'#***************************************************************************************************************************************
Public Function func_Utility_Read_TextFile(strFileName , strWord)

	func_Utility_Read_TextFile = False
	
	
	
	Dim objFso , objFileToRead , strLine
	
	Set objFso = createobject("Scripting.Filesystemobject")
	
	Set objFileToRead = objFso.OpenTextFile(strFileName, ForReading, False)
	
	Do While Not objFileToRead.AtEndOfStream
     	strLine = objFileToRead.ReadLine()
		If Instr(LBound(strLine) , LBound(strWord)) > 0 Then
			func_Utility_Read_TextFile = True
			Exit Do
		End If
	Loop
	objFileToRead.Close
	Set objFso = Nothing
    Set objFileToRead = Nothing        
End Function 
'#***************************************************************************************************************************************
'# Function/Sub Name              : func_Utility_Write_TextFile(strFileName)
'# Purpose                        : To Copy File from one location to another location
'# Input Parameters               :	strFileName
'# Output Parameters              : Boolean Value(True/False)
'# Author                         : Preeti Singh
'# Date Created[dd-mon-yy]    	  : 10-Jan-17
'# Modified By                    :
'# Date Modified[dd-mon-yy]  	  :
'#***************************************************************************************************************************************
Public Function func_Utility_Write_TextFile(strFileName , strContent)

	func_Utility_Write_TextFile = False
	
	Dim objFso , objFileToWrite , objFileToRead , strLine
	
	Set objFso = createobject("Scripting.Filesystemobject")
	
	objFso.CreateTextFile(strFileName)
	
	Set objFileToWrite = objFso.OpenTextFile(strFileName, ForWriting, True)
	objFileToWrite.WriteLine strContent	
	objFileToWrite.Close
	
	Set objFileToRead = objFso.OpenTextFile(strFileName, ForReading, True)
	strLine = objFileToWrite.ReadAll
	
	If strLine <> Empty Then
		func_Utility_Write_TextFile = True
	End If
	
	objFileToRead.Close
	Set objFso = Nothing
    Set objFileToRead = Nothing  
	Set objFileToWrite = Nothing     
End Function 

'#***************************************************************************************************************************************
'# Function/Sub Name              : func_Utility_Write_TextFile(strFileName)
'# Purpose                        : To create excel
'# Input Parameters               :	strFileName , strFilePath
'# Output Parameters              : Boolean Value(True/False)
'# Author                         : Preeti Singh
'# Date Created[dd-mon-yy]    	  : 10-Jan-17
'# Modified By                    :
'# Date Modified[dd-mon-yy]  	  :
'#***************************************************************************************************************************************
Function func_Utility_CreatExcel(strFilePath , strFileName )
	
	
	
	Dim objExcel , objWorkbook	

   	Set objExcel = CreateObject("Excel.Application")
        
	Set objWorkbook = objExcel.Workbooks.Add

        objWorkbook.SaveAs strFilePath & "\" & strFileName

	objWorkbook.Close 
          
	objExcel.Application.Quit

	Set objExcel = Nothing

End Function


