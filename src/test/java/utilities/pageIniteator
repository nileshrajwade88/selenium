package com.pru.utilities;

import org.openqa.selenium.support.PageFactory;
import org.openqa.selenium.support.ui.WebDriverWait;

import com.pru.pageobjects.PruWealth_AdditionalInformationPage;
import com.pru.pageobjects.PruWealth_AssetSelectionPage;
import com.pru.pageobjects.PruWealth_ClientCreationPage;
import com.pru.pageobjects.PruWealth_ConfirmPage;
import com.pru.pageobjects.PruWealth_IncomeAndWithdrawlsPage;
import com.pru.pageobjects.PruWealth_IncomeWithdrawal_Drawdown;
import com.pru.pageobjects.PruWealth_IncomeWithdrawal_PhasedDrawdown;
import com.pru.pageobjects.PruWealth_IncomeWithdrawal_RegularUFPLS;
import com.pru.pageobjects.PruWealth_IncomeWithdrawal_SingleUFPLS;
import com.pru.pageobjects.PruWealth_LaunchLoginPage;
import com.pru.pageobjects.PruWealth_PDF_Comparison;
import com.pru.pageobjects.PruWealth_RegularContributionPage;
import com.pru.pageobjects.PruWealth_SelectNewAccountPage;
import com.pru.pageobjects.PruWealth_ContributionPage;
import com.pru.pageobjects.PruWealth_SingleContributionPage;
import com.pru.pageobjects.PruWealth_TransferPage;
//import com.pru.pageobjects.testing;
//import com.pru.pageobjects.testing;

public class PageInitiator extends TestDataProvider {

	//General
	public WebDriverWait wait;
	
	//Login Page
	public PruWealth_LaunchLoginPage objPruWealth_LaunchLoginPage = null; 
	public PruWealth_ClientCreationPage objPruWealth_ClientCreationPage = null;
	public PruWealth_SelectNewAccountPage objPruWealth_SelectNewAccountPage = null;
	public PruWealth_TransferPage objPruWealth_TransferPage = null;
	public PruWealth_ContributionPage objPruWealth_ContributionPage = null;
	public PruWealth_RegularContributionPage objPruWealth_RegularContributionPage = null;
	public PruWealth_SingleContributionPage objPruWealth_SingleContributionPage = null;
	public PruWealth_AssetSelectionPage objPruWealth_AssetSelectionPage = null;
	public PruWealth_AdditionalInformationPage objPruWealth_AdditionalInformationPage = null;
	public PruWealth_IncomeAndWithdrawlsPage objPruWealth_IncomeAndWithdrawlsPage = null;
	public PruWealth_IncomeWithdrawal_Drawdown objPruWealth_IncomeWithdrawal_Drawdown = null;
	public PruWealth_IncomeWithdrawal_PhasedDrawdown objPruWealth_IncomeWithdrawal_PhasedDrawdown = null;
	public PruWealth_IncomeWithdrawal_RegularUFPLS objPruWealth_IncomeWithdrawal_RegularUFPLS = null;
	public PruWealth_IncomeWithdrawal_SingleUFPLS objPruWealth_IncomeWithdrawal_SingleUFPLS = null;
	
	public PruWealth_ConfirmPage objPruWealth_ConfirmPage = null;
	public PruWealth_PDF_Comparison objPruWealth_PDF_Comparison = null;
	//public testing objtesting = null;
	
	public void initiatePage()
	{
		try{
			if (driver==null){
				System.out.println("driver is null......... in initiate page");
			}
			System.out.println("initiate page starts");

			// objects
			objPruWealth_LaunchLoginPage = PageFactory.initElements(driver, PruWealth_LaunchLoginPage.class);
			objPruWealth_ClientCreationPage = PageFactory.initElements(driver, PruWealth_ClientCreationPage.class);
			objPruWealth_SelectNewAccountPage = PageFactory.initElements(driver, PruWealth_SelectNewAccountPage.class);
			objPruWealth_TransferPage = PageFactory.initElements(driver, PruWealth_TransferPage.class);
			objPruWealth_ContributionPage = PageFactory.initElements(driver, PruWealth_ContributionPage.class);
			objPruWealth_RegularContributionPage = PageFactory.initElements(driver, PruWealth_RegularContributionPage.class);
			objPruWealth_SingleContributionPage = PageFactory.initElements(driver, PruWealth_SingleContributionPage.class);
			objPruWealth_AssetSelectionPage = PageFactory.initElements(driver, PruWealth_AssetSelectionPage.class);
			objPruWealth_AdditionalInformationPage = PageFactory.initElements(driver, PruWealth_AdditionalInformationPage.class);
			objPruWealth_IncomeAndWithdrawlsPage = PageFactory.initElements(driver, PruWealth_IncomeAndWithdrawlsPage.class);
			objPruWealth_IncomeWithdrawal_Drawdown = PageFactory.initElements(driver, PruWealth_IncomeWithdrawal_Drawdown.class);
			objPruWealth_IncomeWithdrawal_PhasedDrawdown = PageFactory.initElements(driver, PruWealth_IncomeWithdrawal_PhasedDrawdown.class);
			objPruWealth_IncomeWithdrawal_RegularUFPLS = PageFactory.initElements(driver, PruWealth_IncomeWithdrawal_RegularUFPLS.class);
			objPruWealth_IncomeWithdrawal_SingleUFPLS = PageFactory.initElements(driver, PruWealth_IncomeWithdrawal_SingleUFPLS.class);
			objPruWealth_ConfirmPage = PageFactory.initElements(driver,PruWealth_ConfirmPage.class);
			objPruWealth_PDF_Comparison = PageFactory.initElements(driver,PruWealth_PDF_Comparison.class);
			//objtesting = PageFactory.initElements(driver,testing.class);
			
			System.out.println("Initiated Successfully");
		}catch(Exception e){
			System.out.println("Error in initiatePage" + e);
		}

	}

}

