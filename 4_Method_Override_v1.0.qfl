   RegisterUserFunc "WebElement", "Click", "sub_register_click"
   RegisterUserFunc "WebButton", "Click", "sub_register_click"
   RegisterUserFunc "WinButton", "Click", "sub_register_click"
   RegisterUserFunc "PbButton", "Click", "sub_register_click"
   RegisterUserFunc "JavaButton", "Click", "sub_register_click"
   'set
   RegisterUserFunc "WinEdit", "Set", "sub_register_set"
   RegisterUserFunc "PbEdit", "Set", "sub_register_set"
   RegisterUserFunc "WebEdit", "Set", "sub_register_set" 
   RegisterUserFunc "WebRadioGroup", "Select", "sub_register_set"
   RegisterUserFunc "WebCheckBox", "Set","sub_register_set"
   RegisterUserFunc "JavaEdit", "Set", "sub_register_set"
   'setsecure
   RegisterUserFunc "WinEdit", "SetSecure", "sub_register_SetSecure" 
  'selectcell	   
   RegisterUserFunc "WebDataWindow", "SelectCell", "sub_register_selectcell" 	
   'setcelldata
