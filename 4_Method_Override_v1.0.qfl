   RegisterUserFunc "WebDataWindow", "SetCellData", "sub_register_setcelldata"
   'select
   RegisterUserFunc "WebList", "Select", "sub_register_select"
   RegisterUserFunc "WebRadioGroup", "Select", "sub_register_select"   
   RegisterUserFunc "WinMenu", "Select", "sub_register_select"
   RegisterUserFunc "WebComboBox", "Select", "sub_register_select"
   RegisterUserFunc "JavaList", "Select", "sub_register_select"
End Function


'# ********************************************************************************************
'# Function/Sub Name        : sub_register_click
'# Purpose                  : to override click method on any object
'# Input Parameters         : Object
'# Output Parameters        : NA
'# Author			        : Kiran Londhe
'# Date Created[dd-mon-yy]  : 29-Dec-2016
'# Modified By				:
'# Date Modified[dd-mon-yy] :
'# ********************************************************************************************

Public Sub sub_register_click(ByRef l_obj)
	On Error Resume Next
		If g_str_micstatus = micFail Then Exit Sub	End If
		If l_obj.Exist(Environment("ShortSyncWait")) Then
			l_obj.click
			Call sub_html_ReportStep(2,micPass,"click on " & l_obj.ToString,"user should be able to click on " & l_obj.ToString,"user is able to click on " & l_obj.ToString)		
		Else
			Call sub_html_ReportStep(2,micFail,"click on " & l_obj.ToString,"user should be able to click on " & l_obj.ToString, "object " & l_obj.ToString & " not found")		
			g_str_micstatus = micFail
		End If
		Set l_obj = Nothing
	On Error GoTo 0
End Sub

'# ********************************************************************************************
'# Function/Sub Name        : sub_register_set
'# Purpose                  : to override set method on any object
'# Input Parameters         : Object,value
'# Output Parameters        : NA
'# Author			        : Kiran Londhe
'# Date Created[dd-mon-yy]  : 29-Dec-2016
'# Modified By				:
'# Date Modified[dd-mon-yy] :
'# ********************************************************************************************

Public Sub sub_register_set(ByRef l_obj,ByVal l_str_value)
	On Error Resume Next
		If g_str_micstatus = micFail Then Exit Sub	End If
		If l_obj.Exist(Environment("ShortSyncWait")) Then
			l_obj.set Trim(l_str_value)
			Call sub_html_ReportStep(2,micPass,"set "& l_str_value  &" in to " & l_obj.ToString,"user should be able to set "& l_str_value  &" in to " & l_obj.ToString,"user is able to set "& l_str_value  &" in to " & l_obj.ToString)
